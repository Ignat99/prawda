/*
# define W_SUB  @Стало{  сущ    Число=Ед Падеж=И }
# define W_ADJ  @Стало{  прил   Число=Ед Падеж=И }
# define W_VERB @Стало{  глагол Форма=Inf }
*/
@Переводики
{
// -------- формы слова -------------------------------------------------------------

   @Шаг { o      @Суффикс = o       o @Было {  сущ   Число=Ед Падеж=И } W_SUB }
   @Шаг { on     @Суффикс = on      o @Было {  сущ   Число=Ед Падеж=В } W_SUB }
   @Шаг { oj     @Суффикс = oj      o @Было {  сущ   Число=Мн Падеж=И } W_SUB }
   @Шаг { ojn    @Суффикс = ojn     o @Было {  сущ   Число=Мн Падеж=В } W_SUB }
   
   @Шаг { a      @Суффикс = a       a @Было {  прил  Число=Ед Падеж=И } W_ADJ }
   @Шаг { an     @Суффикс = an      a @Было {  прил  Число=Ед Падеж=В } W_ADJ }
   @Шаг { aj     @Суффикс = aj      a @Было {  прил  Число=Мн Падеж=И } W_ADJ }
   @Шаг { ajn    @Суффикс = ajn     a @Было {  прил  Число=Мн Падеж=В } W_ADJ }
   
   @Шаг { e      @Суффикс = e       e @Было { наречие } @Стало{ наречие } }
   
   @Шаг { i      @Суффикс = i       i @Было { глагол  Форма=Inf                    } W_VERB }
   @Шаг { as     @Суффикс = as      i @Было { глагол  Форма=Глагол Время=настоящее } W_VERB }
   @Шаг { is     @Суффикс = is      i @Было { глагол  Форма=Глагол Время=прошлое   } W_VERB }
   @Шаг { os     @Суффикс = os      i @Было { глагол  Форма=Глагол Время=будущее   } W_VERB }
   @Шаг { us     @Суффикс = us      i @Было { глагол  Форма=Услов                  } W_VERB }
   @Шаг { u      @Суффикс = u       i @Было { глагол  Форма=Повелит                } W_VERB }

   @Шаг { anta   @Суффикс = anta    i @Было { глагол  Форма=Причастие    Время=настоящее Число=Ед Падеж=И } W_VERB }
   @Шаг { antan  @Суффикс = antan   i @Было { глагол  Форма=Причастие    Время=настоящее Число=Ед Падеж=В } W_VERB }
   @Шаг { antaj  @Суффикс = antaj   i @Было { глагол  Форма=Причастие    Время=настоящее Число=Мн Падеж=И } W_VERB }
   @Шаг { antajn @Суффикс = antajn  i @Было { глагол  Форма=Причастие    Время=настоящее Число=Мн Падеж=В } W_VERB }
   @Шаг { inta   @Суффикс = inta    i @Было { глагол  Форма=Причастие    Время=прошлое   Число=Ед Падеж=И } W_VERB }
   @Шаг { intan  @Суффикс = intan   i @Было { глагол  Форма=Причастие    Время=прошлое   Число=Ед Падеж=В } W_VERB }
   @Шаг { intaj  @Суффикс = intaj   i @Было { глагол  Форма=Причастие    Время=прошлое   Число=Мн Падеж=И } W_VERB }
   @Шаг { intajn @Суффикс = intajn  i @Было { глагол  Форма=Причастие    Время=прошлое   Число=Мн Падеж=В } W_VERB }
   @Шаг { onta   @Суффикс = onta    i @Было { глагол  Форма=Причастие    Время=будущее   Число=Ед Падеж=И } W_VERB }
   @Шаг { ontan  @Суффикс = ontan   i @Было { глагол  Форма=Причастие    Время=будущее   Число=Ед Падеж=В } W_VERB }
   @Шаг { ontaj  @Суффикс = ontaj   i @Было { глагол  Форма=Причастие    Время=будущее   Число=Мн Падеж=И } W_VERB }
   @Шаг { ontajn @Суффикс = ontajn  i @Было { глагол  Форма=Причастие    Время=будущее   Число=Мн Падеж=В } W_VERB }

   @Шаг { ata    @Суффикс = ata     i @Было { глагол  Форма=Прич_пасс    Время=настоящее Число=Ед Падеж=И } W_VERB }
   @Шаг { atan   @Суффикс = atan    i @Было { глагол  Форма=Прич_пасс    Время=настоящее Число=Ед Падеж=В } W_VERB }
   @Шаг { ataj   @Суффикс = ataj    i @Было { глагол  Форма=Прич_пасс    Время=настоящее Число=Мн Падеж=И } W_VERB }
   @Шаг { atajn  @Суффикс = atajn   i @Было { глагол  Форма=Прич_пасс    Время=настоящее Число=Мн Падеж=В } W_VERB }
   @Шаг { ita    @Суффикс = ita     i @Было { глагол  Форма=Прич_пасс    Время=прошлое   Число=Ед Падеж=И } W_VERB }
   @Шаг { itan   @Суффикс = itan    i @Было { глагол  Форма=Прич_пасс    Время=прошлое   Число=Ед Падеж=В } W_VERB }
   @Шаг { itaj   @Суффикс = itaj    i @Было { глагол  Форма=Прич_пасс    Время=прошлое   Число=Мн Падеж=И } W_VERB }
   @Шаг { itajn  @Суффикс = itajn   i @Было { глагол  Форма=Прич_пасс    Время=прошлое   Число=Мн Падеж=В } W_VERB }
   @Шаг { ota    @Суффикс = ota     i @Было { глагол  Форма=Прич_пасс    Время=будущее   Число=Ед Падеж=И } W_VERB }
   @Шаг { otan   @Суффикс = otan    i @Было { глагол  Форма=Прич_пасс    Время=будущее   Число=Ед Падеж=В } W_VERB }
   @Шаг { otaj   @Суффикс = otaj    i @Было { глагол  Форма=Прич_пасс    Время=будущее   Число=Мн Падеж=И } W_VERB }
   @Шаг { otajn  @Суффикс = otajn   i @Было { глагол  Форма=Прич_пасс    Время=будущее   Число=Мн Падеж=В } W_VERB }

   @Шаг { ante   @Суффикс = ante    i @Было { глагол  Форма=Деепричастие Время=настоящее Число=Ед Падеж=И } W_VERB }
   @Шаг { inte   @Суффикс = inte    i @Было { глагол  Форма=Деепричастие Время=прошлое   Число=Ед Падеж=И } W_VERB }
   @Шаг { onte   @Суффикс = onte    i @Было { глагол  Форма=Деепричастие Время=будущее   Число=Ед Падеж=И } W_VERB }

// ----- переходы в другую часть речи -------------------------------------

@Шаг {   oa  @Суффикс = o a  @Было { сущ     } @Стало { прил    } @Переводик { sa_1 sa_2 sa_3 sa_4 } }
@Шаг {   oi  @Суффикс = o i  @Было { сущ     } @Стало { глагол  } @Переводик { sv1 /* (все sv) */ } }   
@Шаг 
{   anto @Суффикс = anto i  @Было { сущ     } @Стало { глагол  } @Переводик { sv1 /* (все sv) */ }
    @Перевод
    {  
       @Структура1 гр_сущ_п20( число Число, падеж Падеж ) =
		   глагол ( %Inf, @0, @0, @0 ) ;
       @Структура1 гр_сущ_п20( род &Род, душа Душа, падеж &Падеж, число &Число ) =
                   ?прил[тот]( Род, %О, Падеж, Число )
		   @0[,] @0[кто]
	           !глагол( %Несов, %глагол, %3е, %Ед, %настоящее, @0, %Актив, @0 ) ;
    }
}

@Шаг { oe  @Суффикс = o e  @Было { сущ     } @Стало { наречие }  /* все обратные ds */ }

@Шаг { ao  @Суффикс = a o  @Было { прил    } @Стало { сущ     } @Переводик { as1 as2 as3 } }
@Шаг { ai  @Суффикс = a i  @Было { прил    } @Стало { глагол  } @Переводик { sv } }
@Шаг { ae  @Суффикс = a e  @Было { прил    } @Стало { наречие } @Переводик { ad } }

@Шаг 
{   io  @Суффикс = i o  @Было { глагол  } @Стало { сущ     } /* @Переводик { обратный sv1 } */
    @Перевод
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_п2( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   ?глагол[делать]( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж )
		   !сущ( &Р, &Д, %И, %Ед ) ;
    }
}
@Шаг 
{   ia  @Суффикс = i a  @Было { глагол  } @Стало { прил    }  /* Нет такой связки */
    @Перевод
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_п3( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   ?глагол[быть]( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж )
		   !прил( &М, &Н, %Т, %Ед ) ;
    }        
}
@Шаг 
{   ie  @Суффикс = i e  @Было { глагол  } @Стало { наречие }  }
@Шаг 
{   eo  @Суффикс = e o  @Было { наречие } @Стало { сущ     }  
        @Переводик {  ds_1 ds_2 ds_3 ds_4 ds_5 ds_6 ds_7 ds_8 ds_9 ds_10 ds_11 ds_12 ds_13 ds_14 ds_15 ds_18 }  
           /* не могу придумать структуру */
}
@Шаг 
{   ea  @Суффикс = e a  @Было { наречие } @Стало { прил    }  @Переводик {  da /* обратное ad */  }  
           /* не могу придумать структуру */
}
@Шаг 
{   ei  @Суффикс = e i  @Было { наречие } @Стало { глагол  }  @Переводик {  dv  }  }

// ----- суффиксы ----------------------------------------------------------

// ACX  плохое качество, негодность, бездарность.
@Шаг
{  acx  @Суффикс = acxo o @Было { сущ    } @Стало { сущ    } 
   @Перевод
   {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	          сущ( &Число, &Падеж );
      @Структура1 гр_сущ_п1( род Род, душа Душа, падеж &Падеж, число &Число ) =
                  ?прил[плохой]( Род, Душа, Падеж, Число )
	          !сущ( &Род, &Душа, Падеж, Число ) ;
   }
}
// AJX  (конкретное) вещь "красивый"->"украшение" "иметь"->"имущество"
@Шаг
{   ajx1 @Суффикс = ajxo a @Было { сущ    } @Стало { прил   } @Переводик { sa_1 /* и только оно */ } 
    @Перевод
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п21( %С, %Н, падеж &Падеж, число &Число ) =
                   ?сущ[свойство]( &Род, &Душа, Падеж, Число )
		   @0[быть]
		   !прил( %М, %Н, %Т, %Ед );
    }   
}
@Шаг
{   ajx2 @Суффикс = ajxo i @Было { сущ    } @Стало { глагол } }
// AN   член  группы, житель местности, приверженец  доктрины, особы которые принадлежат месту и т.п.                                                                        
@Шаг
{   an   @Суффикс = ano  o @Было { сущ    } @Стало { сущ    } @Переводик { ss4 ss4_1 } }
// AR   целое или собрание  многих samspecaj дел.                  
@Шаг
{   ar   @Суффикс = aro  o @Было { сущ    } @Стало { сущ    } @Переводик { ss11 } }
// CXJ  делают _ ласковые имена  мужчин.
@Шаг 
{   cxj  @Суффикс = cxjo o @Было { сущ    } @Стало { сущ    } 
    @Перевод
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п1( род Род, душа Душа, падеж &Падеж, число &Число ) =
                   ?прил[милый]( Род, Душа, Падеж, Число )
	           !сущ( &Род, &Душа, Падеж, Число ) ;
    }
}
// EC   качество или состояние  кого-то или чего-то "краснота" "состояние краснота"
@Шаг
{   ec   @Суффикс = eco  o @Было { сущ    } @Стало { сущ    } }
// EG   увеличительный суффикс                                      
@Шаг 
{   eg   @Суффикс = ego  o @Было { сущ    } @Стало { сущ    } @Переводик { ss1 }
    @Перевод
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п1( род Род, душа Душа, падеж &Падеж, число &Число ) =
                   ?прил[большой]( Род, Душа, Падеж, Число )
	           !сущ( &Род, &Душа, Падеж, Число ) ;
    }
}
// EJ   место, где делают это "учиться"->"школа"                    
@Шаг
{   ej   @Суффикс = ejo  i @Было { сущ    } @Стало { глагол } @Переводик { sv2_1 sv2_2 sv2_3  } }
// END  "необходимо, чтобы это было сделано"
@Шаг
{   end  @Суффикс = endo o @Было { сущ    } @Стало { сущ    } }
// ER   "маленькая часть чего-то"  "снег"->"снежинка"
@Шаг
{   er   @Суффикс = ero  o @Было { сущ    } @Стало { сущ    } @Переводик { ss10_1 } 
    @Перевод   // новая структура
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п12( род Род, душа Душа, падеж &Падеж, число &Число ) =
                   прил[маленький]( %Ж, %Н, Падеж, Число )
	           ?сущ[часть]( &Род, &Душа, Падеж, Число )
		   !сущ( &Род1, &Душа1, %Р, %Ед ) ;
    }
}
// ESTR "руководитель чего-то" "империя"->"император"
@Шаг
{   estr @Суффикс = estro o @Было { сущ    } @Стало { сущ    }
    @Перевод   // новая структура
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п11( род Род, душа Душа, падеж &Падеж, число &Число ) =
	           ?сущ[руководитель]( &Род, &Душа, Падеж, Число )
		   !сущ( &Род1, &Душа1, %Р, %Ед ) ;
    }
}
