// UJ   что-то предназначенное для содержания чего-то "пепел"->"пепельница"
@Шаг
{   uj   @Суффикс = ujo  o @Было { сущ    } @Стало { сущ    } }
// UL   особа с какой-то характеристикой "толстый"->"толстяк"
@Шаг
{   ul   @Суффикс = ulo  a @Было { сущ    } @Стало {  прил  } }
// AD   длительное действие.
@Шаг
{   ad   @Суффикс = adi  i @Было { глагол } @Стало { глагол } 
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_п4( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   @0[долго] !?глагол( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж ) ;
    }   
}
// IG   действие приводящее к этому результату "острый"->"точить"
@Шаг
{   ig   @Суффикс = igi  a @Было { глагол } @Стало { прил   } 
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_п3( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   ?глагол[делать]( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж )
		   !прил( %М, %Н, %Т, %Ед );
    }
}
// IND  заслуживает быть сделанным "читать"->"следует быть прочитанным"
@Шаг {  ind  @Суффикс = indi i @Было { глагол } @Стало { глагол } }
// UM   нет определенного значения "лист"->"листать" "глаз"->"глазеть" "рука"->"обнимать"
@Шаг {  um   @Суффикс = umi  o @Было { глагол } @Стало {  сущ   } }
// IGX  переход к новому состоянию "острый"->"заостряться"
@Шаг
{   igx  @Суффикс = igxi a @Было { глагол } @Стало { прил   }
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_п3( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   ?глагол[делаться]( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж )
		   !прил( %М, %Н, %Т, %Ед );
    }   
}
// EBL  может быть сделанный.
@Шаг {  ebl  @Суффикс = ebla i @Было { прил   } @Стало { глагол } }
// EM   тенденция делать это "лгать"->"лживый"
@Шаг {  em   @Суффикс = ema  i @Было { прил   } @Стало { глагол } }
// OBL  суффикс умножения -  "два"->"двукратный" "двойной"
@Шаг {  obl  @Суффикс = obl - @Было { прил   } @Стало { числительное } }
// ON   суффикс деления      "два"->"половина"
@Шаг {  on   @Суффикс = on   - @Было { числительное } @Стало { числительное } }
// OP   группа с уверенным числом членов "десять"->"десятеро"
@Шаг {  op   @Суффикс = op   - @Было { числительное } @Стало { числительное } }

// ----- приставки ----------------------------------------------------------

// BO   = родство в результате брака "сын"->"зять"
@Шаг {  bo   @Приставка = bo   - @Было { сущ    } @Стало { сущ    } }
// CXEF = наиболее важный "епископ"->"архиепископ"
@Шаг
{   cxef @Приставка = cxef - @Было { сущ    } @Стало { сущ    } 
    @Перевод
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п1( род Род, душа Душа, падеж &Падеж, число &Число ) =
                   ?прил[главный]( Род, Душа, Падеж, Число )
	           !сущ( &Род, &Душа, Падеж, Число ) ;
    }
}
// DIS  = действовать в разных направлениях "бежать"->"разбегаться"
@Шаг
{   dis  @Приставка = dis  - @Было { глагол } @Стало { глагол } }
// EK   = начала действия, внезапное действие "спать"->"заснуть"
@Шаг
{   ek   @Приставка = ek   - @Было { глагол } @Стало { глагол } 
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_п5( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   ?глагол[начать]( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж )
		   !глагол        ( %Несов, %Inf, @0, @0, @0, @0, @0, @0 ) ;
    }
}
// EKS  = обозначение бывшей роли "министр"->"эксминистр"
@Шаг
{   eks  @Приставка = eks  - @Было { сущ    } @Стало { сущ    }
    @Перевод
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_п1( род Род, душа Душа, падеж &Падеж, число &Число ) =
                   ?прил[бывший]( Род, Душа, Падеж, Число )
	           !сущ( &Род, &Душа, Падеж, Число ) ;
    }   
}
// GE   = оба человека образующих пару "отец"->"родители"
@Шаг
{   ge   @Приставка = ge   - @Было { сущ    } @Стало { сущ    } }
// MAL  = противоположное значение "хороший"->"плохой"
@Шаг
{   mal  @Приставка = mal  - @Было { сущ    } @Стало { сущ    } 
    @Перевод
    {  @Структура1 гр_сущ_п0( число Число, падеж Падеж ) =
	           сущ( &Число, &Падеж );
       @Структура1 гр_сущ_н1( род Род, душа Душа, падеж &Падеж, число &Число ) =
                   @0[не] ?!сущ( &Род, &Душа, Падеж, Число ) ;
    }
}
@Шаг
{   mal  @Приставка = mal  - @Было { прил   } @Стало { прил   } @Переводик { aan  aa_5 } 
    @Перевод
    {  @Структура1 гр_прил_п0( число Число, падеж Падеж ) =
	           прил( &Число, &Падеж );
       @Структура1 гр_прил_п6( род &Род, душа &Душа, падеж &Падеж, число &Число ) =
                   @0[не] ?!прил( Род, Душа, Падеж, Число ) ;
    }
}
@Шаг
{   mal  @Приставка = mal  - @Было { глагол } @Стало { глагол } 
    {  @Структура1 гр_глагол_пп( форма Форма, время Время, число Число, падеж Падеж ) =
                   глагол( &Форма, &Время, &Число, &Падеж ) ;
       @Структура1 гр_глагол_н1( совершенность &СС, форма &Форма, лицо &Лицо, число &Число,
                                 время &Время, род &Род, залог &Залог, падеж &Падеж ) =
                   @0[не] ?!глагол( СС, Форма, Лицо, Число, Время, Род, Залог, Падеж ) ;
    }
}
@Шаг
{   mal  @Приставка = mal  - @Было { наречие} @Стало { наречие} 
    {  @Структура1 гр_наречие_п0( смысл_наречия Смысл ) =
	           наречие( &Смысл );
       @Структура1 гр_наречие_н1( смысл_наречия Смысл ) =
                   @0[не] ?!наречие( &Смысл ) ;
    }
}
// MIS  = сделать что-то плохо или с ошибкой "шагать"->"оступаться"
@Шаг {  mis  @Приставка = mis  - @Было { глагол } @Стало { глагол } }
// PRA  = предыдущий очень долгого времени "человек"->"прачеловек"
@Шаг {  pra  @Приставка = pra  - @Было { сущ    } @Стало { сущ    } }
// RE   = делать это еще раз или в обратном направлении "читать"->"перечитывать" "давать"->"возвращать"
@Шаг {  re   @Приставка = re   - @Было { глагол } @Стало { глагол } }

